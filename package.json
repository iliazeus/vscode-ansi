{
  "name": "vscode-ansi",
  "publisher": "iliazeus",
  "displayName": "ANSI Colors",
  "description": "ANSI color styling for text documents",
  "version": "1.0.3",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/iliazeus/vscode-ansi"
  },
  "bugs": {
    "url": "https://github.com/iliazeus/vscode-ansi/issues",
    "email": "ilia.pozdnyakov@ya.ru"
  },
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.45.0"
  },
  "categories": [
    "Themes",
    "Visualization"
  ],
  "keywords": [
    "ansi",
    "escapes",
    "colors",
    "logs"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "iliazeus.vscode-ansi.showPretty",
        "title": "Show as ANSI",
        "icon": "$(open-preview)"
      },
      {
        "command": "iliazeus.vscode-ansi.showPrettyToSide",
        "title": "Show as ANSI to the Side",
        "icon": "$(open-preview)"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "iliazeus.vscode-ansi.showPrettyToSide",
          "alt": "iliazeus.vscode-ansi.showPretty",
          "when": "editorLangId == ansi",
          "group": "navigation"
        }
      ]
    },
    "keybindings": [
      {
        "command": "iliazeus.vscode-ansi.showPretty",
        "key": "shift+ctrl+v",
        "mac": "shift+cmd+v",
        "when": "editorLangId == ansi"
      },
      {
        "command": "iliazeus.vscode-ansi.showPrettyToSide",
        "key": "ctrl+k v",
        "mac": "cmd+k v",
        "when": "editorLangId == ansi"
      }
    ],
    "languages": [
      {
        "id": "ansi",
        "aliases": [
          "ANSI text"
        ],
        "extensions": [
          ".ans",
          ".ansi"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "test": "node ./out/test/runTest.js",
    "deploy": "vsce publish --yarn"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.45.0",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "eslint": "^7.11.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.1",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3",
    "vsce": "^1.81.1",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "ansicolor": "^1.1.93"
  }
}
